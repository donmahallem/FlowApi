{
    "name": "@donmahallem/flowapi",
    "version": "1.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "mocha --timeout 10000 --check-leaks -r ts-node/register src/**/*.spec.ts",
        "start": "npm run build:live",
        "build:live": "nodemon",
        "build": "node ./node_modules/typescript/bin/tsc --outdir ./dist/ --baseUrl ./src/",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "test-ci_old": "nyc ts-mocha -p tsconfig.json -- --opts mocha.opts",
        "test-ci": "nyc mocha --opts mocha.opts"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^10.12.18",
        "@types/set-cookie-parser": "0.0.3",
        "nodemon": "^1.18.0",
        "request-debug": "^0.2.0",
        "ts-node": "^7.0.1",
        "typescript": "^3.2.2"
    },
    "nyc": {
        "include": [
            "src/*.ts",
            "src/**/*.ts"
        ],
        "exclude": [
            "typings",
            "src/**/*.spec.ts",
            "**/*.d.ts"
        ],
        "extension": [
            ".ts"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json",
            "html",
            "text-summary",
            "lcov"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true
    },
    "dependencies": {
        "jsonschema": "^1.2.4",
        "querystring": "^0.2.0",
        "request": "^2.88.0",
        "set-cookie-parser": "^2.2.1"
    },
    "files":[
        "dist/*"
    ]
}
